{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "supportedLocales": [
    "en",
    "pl"
  ],
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-rc.3",
    "@chakra-ui/theme": "^1.0.0-rc.3",
    "@chakra-ui/theme-tools": "^1.0.0-rc.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.159",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-router-dom": "^5.1.5",
    "bootstrap": "^4.5.2",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "node-sass": "^7.0.0",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-icons": "^3.0.0",
    "react-intl": "^5.6.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test --passWithNoTests",
    "build": "react-scripts build",
    "intl:extract": "formatjs extract -- 'src/**/*.{ts,tsx}' --out-file src/lang/i18n/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile": "formatjs compile -- src/intl/lang/en.json --ast --out-file src/intl/i18n/en.json"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "formatjs"
    ],
    "rules": {
      "formatjs/no-offset": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@formatjs/cli": "^2.7.3",
    "babel-plugin-react-intl": "^7.9.4",
    "babel-preset-react-app": "^9.1.2",
    "eslint-plugin-formatjs": "^2.6.3",
    "formatjs": "^0.1.1"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "flow": false,
          "typescript": true
        }
      ]
    ],
    "plugins": [
      [
        "react-intl",
        {
          "extractFromFormatMessageCall": true,
          "idInterpolationPattern": "[sha512:contenthash:base64:6]",
          "ast": true
        }
      ]
    ]
  }
}
